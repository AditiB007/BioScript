main:
	a1[0] = dispense(droplet_a)
	b1[0] = dispense(droplet_b)
	ab1[0] = mix(a1[0], b1[0]) @ 10.0SECOND usein 10.0SECOND
bsbbr_2_h:
	PHI x1 = {x0,x3}
	PHI REPEAT_101 = {REPEAT_100,REPEAT_102}
	PHI ab2 = {ab1,ab2}
	if REPEAT_101[0] > 0	|	true: jump bsbbr_3_t	|	false: jump bsbbr_4_f
bsbbr_3_t:
	heat(ab2[0]) @ 90.0CELSIUS @ 5.0SECOND usein 20.0SECOND
	x2[0] = detect(mod, ab2[0]) @ 10.0SECOND
	heat(ab2[0]) @ 90.0CELSIUS @ 2.0SECOND usein 15.0SECOND
	x3[0] = detect(mod, ab2[0]) @ 10.0SECOND
	REPEAT_102[0] = REPEAT_101[0] - 1
bsbbr_4_f:
	dispose(ab2[0])
	NOP
